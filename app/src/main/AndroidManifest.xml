<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.apadok.emrpreventive">

    <!-- Application Permissions -->
    <uses-permission android:name="android.permission.INTERNET" />  <!-- Allow this application to access internet -->
    <uses-sdk tools:overrideLibrary="androidx.security" /> <!-- Enable SharedPrefs Encryption for SDK >23 -->
    <queries>
        <package android:name="com.whatsapp" /> <!-- Allow this application to access whatsapp directly -->
        <package android:name="com.google.android.apps.maps" /> <!-- Allow this application to access maps directly -->
    </queries>

    <!-- Application Properties -->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EMRPreventive"
        tools:ignore="UnusedAttribute">
        <activity
            android:name=".socketchat.MessageListActivity"
            android:exported="false" />
        <activity
            android:name=".socketchat.SocketChatActivity"
            android:exported="false" />
        <activity
            android:name=".user.SignupActivity"
            android:exported="false" />
        <activity
            android:name=".consult.NearestClinicActivity"
            android:exported="false" />
        <activity
            android:name=".consult.ConsultActivity"
            android:exported="false" />
        <activity
            android:name=".user.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".chatbot.TestChatbotActivity"
            android:exported="false" />
        <activity
            android:name=".screening.KebugaranScreeningActivity"
            android:exported="false" />
        <activity
            android:name=".screening.KebugaranScreeningResultActivity"
            android:exported="false" />
        <activity
            android:name=".kebugaranhistory.KebugaranHistoryActivity"
            android:exported="false" />
        <activity
            android:name=".kebugaranhistory.KebugaranHistoryDetailActivity"
            android:exported="false" />
        <activity
            android:name=".encyclopedia.EncyclopediaActivity"
            android:exported="false" />
        <activity
            android:name=".encyclopedia.EncyclopediaDetailActivity"
            android:exported="false" />
        <activity
            android:name=".encyclopedia.EncyclopediaInfografisActivity"
            android:exported="false" />
        <!-- Use Transparent Background for This Activity -->
        <activity
            android:name=".encyclopedia.EncyclopediaInfografisPhotoViewActivity"
            android:exported="false"
            android:theme="@style/Theme.AppCompat.Translucent"/>
        <activity
            android:name=".screening.ScreeningActivity"
            android:exported="false" />
        <activity
            android:name=".screeninghistory.ScreeningHistoryDetailActivity"
            android:exported="false" />
        <activity
            android:name=".screeninghistory.ScreeningHistoryActivity"
            android:exported="false" />
        <activity
            android:name=".screening.ScreeningResultActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>